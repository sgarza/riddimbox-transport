{
  "env": {
    "browser": true,
    "commonjs": true,
    "es6": true,
    "node": true,
    "jest": true,
  },
  "plugins": ["babel", "jest", ],
  "extends": ["eslint:recommended", "plugin:prettier/recommended", "plugin:jest/recommended"],
  "parserOptions": {
    "sourceType": "module",
    "allowImportExportEverywhere": false,
    "ecmaFeatures": {
      "globalReturn": false,
    },
    "babelOptions": {
      "configFile": "./.babelrc",
    },
  },
  "settings": {},
  "rules": {
    "linebreak-style": ["error", "unix"],
  },
  "parser": "babel-eslint",
}
